<h1>Trackpoints#index</h1>

<table>
<% for trackpoint in @trackpoints %>
	<tr>
		<td><%=h trackpoint.heart_rate %></td>
		<td><%=h trackpoint.latitude %></td>
		<td><%= link_to 'Show', activity_trackpoint_path(@activity, trackpoint) %></td>
		<td><%= link_to 'Edit', edit_activity_trackpoint_path(@activity, trackpoint) %></td>
	</tr>
<% end %>
</table>	

<%= link_to 'New trackpoint', new_activity_trackpoint_path(@activity) %>
<%= link_to 'Back to Activity', @activity %>