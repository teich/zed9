<% form_for(@workout, :html => { :multipart => true }) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
	<p> 
		<%= f.label "Upload your device file:" %>
		<%= file_field_tag "device_file" %> 
	</p> 
  <p>
    <%= f.submit "Save" %>
  </p>
<% end %>